{
    "name": "Tab Deduplicator",
    "version": "1.1",
    "description": "Displays the current number of open tabs, open windows or both",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs"
    ],
    "icons": {
        "128": "icon128.png"
    },
    "incognito": "split",
    "page_action": {
        "default_icon": {
            "128": "icon128.png"
        },
        "default_title": "Duplicate Tab"
    },
    "manifest_version": 2
}
